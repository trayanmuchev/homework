		arr[2] = arr[2] - arr[3];
		arr[4] = arr[4] * arr[5];
		arr[5] = arr[4] / arr[5];
		arr[4] = arr[4] / arr[5];
		/*for (int i = 0; i < 7; i++) {
			System.out.print(arr[i] + " ");
		}*/
		System.out.println(Arrays.toString(arr));

