import java.util.Arrays;

public class Task_4 {

	public static void main(String[] args) {
		
		int[][] twoDimArr = { { 1, 2, 3 }, { 20, 52, 78 }, { 14, 8, 7 } };
		
		for (int row1 = 0, col2 = 0; row1 < twoDimArr.length && col2 < twoDimArr[0].length; row1++, col2++) {
			for (int col1 = 0, row2 = twoDimArr.length; col1 < twoDimArr[0].length && row2 > 0; col1++, row2--) {
				twoDimArr[row1][col1] = twoDimArr[row2][col2];
			}
			
			

	}
		for (int r = 0; r < twoDimArr.length; r++) {
			for (int c = 0; c < twoDimArr[0].length; c++) {
				System.out.println(twoDimArr[r][c]);
			}

}
	
}
}
