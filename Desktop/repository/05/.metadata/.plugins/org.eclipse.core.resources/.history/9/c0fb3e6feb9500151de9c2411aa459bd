
public class sample3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(checkCube(5));

	}
	
	static boolean checkCube(int n){
		boolean isCube = false;
		for(int i = 2; i <= n>>3; i++){
			if(n/(i<<3) == 1){
				isCube = true;
			}
		}
		return isCube;
	}

}
