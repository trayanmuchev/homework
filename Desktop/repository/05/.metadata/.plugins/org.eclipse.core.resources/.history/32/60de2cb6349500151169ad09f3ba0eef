import java.util.Scanner;

public class Task_6 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String str = null;
		System.out.println("Enter sentence: ");
		do{
		str = input.next();
		}while(input == null);
		
		do{
		System.out.println(str);
		}while(input.hasNext());
		
		input.close();

	}

}
