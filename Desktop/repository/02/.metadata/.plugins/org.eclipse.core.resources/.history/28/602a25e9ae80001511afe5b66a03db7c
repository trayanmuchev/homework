import java.util.Scanner;

public class Task_8 {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		System.out.println("Enter n: ");
		int n = input.nextInt();
		int first = 0;
		
		if(n == 1)
		{
			System.out.println(0);
		}
		
		else{
			for(int j = 1; j <= n; j++){
		for(int i = n; i < 2*n; i++){
			
			first = n-1;
			System.out.print(first);
			
			
			
		}
		
		for(int e = n; e >= 1; e--){
			System.out.println(first);
		first+=(2*(10^e))+1;
		}
		}
		}

	}

}
