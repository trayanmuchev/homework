import java.util.Scanner;

public class Task_8 {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		System.out.println("Enter n: ");
		int n = input.nextInt();
		int first = 0;
		int rest = 0;
		int e;
		
		if(n == 1)
		{
			System.out.println(0);
		}
		
		else{
		
			first = n-1;
		for(int i = 1; i <= n; i++){
			System.out.print(first);
			
		}
		for(int j = 1; j <= n; j++){
			for(e = 0; e <= n; /*e+=2*/){
				
			
			}
			
			System.out.print(first+e);
		}
		}

	}

}
