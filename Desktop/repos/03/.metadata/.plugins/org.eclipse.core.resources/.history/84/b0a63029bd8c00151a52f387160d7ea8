
public class Task_16 {

	public static void main(String[] args) {
		
		double[] arr={ -1.12,-2.43,3.1,4.2,6.4,-7.5,8.5,-110,212,341};
		double[] arr1=new double[10];
		for(int i=0;i<10;i++){
			System.out.print(arr[i]+" ");
		}
		
		double sum=0;
		int br=0;
		
		for(int i=0;i<10;i++){
			if(arr[i]<-0.231){
				arr1[i]=(i*i)+41.25;
			}
			else{
				arr1[i]=arr[i]*i;
			}
		
		}
		
		System.out.println();
		System.out.println("Nov masiv");
		for(int i=0;i<10;i++){
			System.out.print(arr1[i]+" ");
		}
	

	}

}
