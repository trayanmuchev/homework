		double[] arr1=new double[10];
		for(int i=0;i<10;i++){
			System.out.print(arr[i]+" ");
		}
		double sum=0;
		int br=0;
		for(int i=0;i<10;i++){
			if(arr[i]<-0.231){
				arr1[i]=(i*i)+41.25;
			}
			else{
				arr1[i]=arr[i]*i;
			}
			
		}
		System.out.println();
		System.out.println("Star masiv");
		for(int i=0;i<10;i++){
