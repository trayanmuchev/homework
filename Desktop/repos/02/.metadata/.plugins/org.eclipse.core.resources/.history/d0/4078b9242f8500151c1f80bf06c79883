import java.util.Scanner;

public class Task_24 {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		int n;
		int dig;
		do{
		System.out.println("Enter N [10 ... 30000]: ");
		n = input.nextInt();
		}while(n < 10 || n > 30000);
		
		int num = n;
		
		 int rev = 0;
		 while (num > 0)
		 {
		      dig = num % 10;
		      rev = rev * 10 + dig;
		      num = num / 10;
		 }

	}

}
